{
    "name": "ang3/odoo-bundle",
    "type": "symfony-bundle",
    "description": "Symfony bundle to manage your Odoo databases.",
    "keywords": ["odoo", "api", "client", "bundle", "dbal", "orm", "database", "persistence"],
    "license": "MIT",
    "authors": [
        {
            "name": "Joanis ROUANET",
            "email": "joanis.ang3@gmail.com",
            "homepage": "https://github.com/Ang3"
        }
    ],
    "autoload": {
        "psr-4": {
            "Ang3\\Bundle\\OdooBundle\\": "src/"
        }
    },
    "require": {
        "php": "^7.4",
        "ang3/php-odoo-api-client": "^5.1.2",
        "ang3/php-odoo-orm": "^0.1",
        "symfony/cache": ">=3.4.0",
        "symfony/dependency-injection": ">=3.4.0",
        "symfony/config": ">=3.4.0",
        "symfony/http-kernel": ">=3.4.0",
        "symfony/security-core": ">=3.4.0",
        "symfony/validator": ">=3.4.0",
        "symfony/expression-language": ">=3.4.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "ang3/php-dev-binaries": "^1.0"
    }
}
